CREATE TABLE `hotel` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键',
  `sno` varchar(255) DEFAULT NULL COMMENT '酒店编号',
  `address` varchar(255) DEFAULT NULL COMMENT '酒店地址',
  `name` varchar(255) DEFAULT NULL COMMENT '酒店名称',
  `url` varchar(255) DEFAULT NULL COMMENT '酒店链接',
  `score` varchar(255) DEFAULT NULL COMMENT '酒店评分',
  `star` varchar(255) DEFAULT NULL COMMENT '酒店星级',
  `short_name` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '酒店简称',
  `star_desc` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '酒店型号（舒适型，高档型）',
  `dp_count` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '参与点赞用户',
  `dp_score` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL COMMENT '好评率',
  `is_single_rec` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci DEFAULT NULL,
  `lat` varchar(255) DEFAULT NULL COMMENT '纬度',
  `lon` varchar(255) DEFAULT NULL COMMENT '经度',
  `tel` varchar(255) DEFAULT NULL COMMENT '电话',
  `create_time` date DEFAULT NULL COMMENT '创建时间',
  `update_time` date DEFAULT NULL COMMENT '更新时间',
  `type` varchar(255) DEFAULT NULL COMMENT '类型',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=12704 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='酒店信息表';

CREATE TABLE `hotel_room` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键',
  `room_id` varchar(255) DEFAULT NULL COMMENT '房间主键',
  `room_type` varchar(255) DEFAULT NULL COMMENT '房型号',
  `bed_count` varchar(255) DEFAULT NULL COMMENT '床数量',
  `bed_type` varchar(255) DEFAULT NULL COMMENT '床型',
  `break_fask` varchar(255) DEFAULT NULL COMMENT '是否有早餐',
  `services` varchar(255) DEFAULT NULL COMMENT '服务设施',
  `peoples` varchar(255) DEFAULT NULL COMMENT '入住人数',
  `data_policy` varchar(255) DEFAULT NULL COMMENT '促销优惠政策',
  `room_price` varchar(255) DEFAULT NULL COMMENT '房间真实价格',
  `residue_room` varchar(255) DEFAULT NULL COMMENT '剩余房间',
  `book_way` varchar(255) DEFAULT NULL COMMENT '预订方式',
  `area` varchar(255) DEFAULT NULL COMMENT '面基',
  `floor_level` varchar(255) DEFAULT NULL COMMENT '楼层',
  `data_price` varchar(255) DEFAULT NULL COMMENT '原价',
  `data_pricedisplay` varchar(255) DEFAULT NULL COMMENT '吓人价',
  `hotel_id` varchar(255) DEFAULT NULL COMMENT '所属酒店id',
  `hotel_sno` varchar(255) DEFAULT NULL COMMENT '所属酒店编号',
  `hotel_address` varchar(255) DEFAULT NULL COMMENT '所属酒店地址',
  `hotel_name` varchar(255) DEFAULT NULL COMMENT '所属酒店名称',
  `lat` varchar(255) DEFAULT NULL COMMENT '所属酒店经纬度纬度',
  `lon` varchar(255) DEFAULT NULL COMMENT '所属酒店经纬度经度',
  `tel` varchar(255) DEFAULT NULL COMMENT '所属酒店电话',
  `create_time` varchar(255) DEFAULT NULL COMMENT '创建时间',
  `update_time` varchar(255) DEFAULT NULL COMMENT '更新时间',
  `type` varchar(255) DEFAULT NULL COMMENT '类型',
  `source` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=88056 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='酒店房间详细表';